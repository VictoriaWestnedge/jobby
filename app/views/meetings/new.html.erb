<div class="container">
  <div class="row mt-3 mb-4">
    <div class="col">
      <%= simple_form_for @meeting do |f| %>
      <%= f.input :name, label: t("description") %>
      <%= f.input :start_time, label: t("start_t_d")%>
      <%= f.input :end_time, label: t("end_t_d")%>
      <%= f.button :submit, t("createmeeting") %>

      <% end %>
    </div>
  </div>
</div>
